/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     public int val;
 *     public ListNode next;
 *     public ListNode(int val=0, ListNode next=null) {
 *         this.val = val;
 *         this.next = next;
 *     }
 * }
 */
public class Solution {
    public ListNode DeleteDuplicates(ListNode head) {
        
        if(head == null)
            return null;
        
        ListNode temp = head;
        ListNode prev = null;
        
        HashSet<int> hash1 = new HashSet<int>();
        
        while(temp != null)
        {
            if(hash1.Contains(temp.val))
            {
                prev.next = temp.next;
                temp = temp.next;
            }
            else 
            {
                hash1.Add(temp.val);
                prev = temp;
                temp = temp.next;
            }
            
        }
        
        return head;
    }
}
