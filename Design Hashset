public class MyHashSet {
    
    public Node head, temp;
    
    public class Node{
        
        public int data;
        public Node next;
        public Node(int d)
        {
            data = d;
            next = null;
        }
    }

    public MyHashSet() {
        
    }
    
    public void Add(int key) {
        
        if(!Contains(key))
        {
            Node n = new Node(key);
             if(head == null)
             {
                 head = n;
                 temp = n;
                 return;
             }
            
                temp.next = n;
                temp = n;
        }
        
    }
    
    public void Remove(int key) {
        
        if(head == null)
            return;
        
        Node temp = head;
        Node prev = null;
        while(temp != null)
        {
            if(temp.data == key)
            {
                if(prev == null)
                {
                   head = head.next; 
                    
                }
                else{
                    prev.next = temp.next;
                    
                }    
                break;
            }
            prev = temp;
            temp = temp.next;
        }
        
    }
    
    public bool Contains(int key) {
        
        Node curr = head;
        
        while(curr != null)
        {
            if(curr.data == key)
            {
                return true;
            }
            curr = curr.next;
        }
        
        return false;
    }
}

/**
 * Your MyHashSet object will be instantiated and called as such:
 * MyHashSet obj = new MyHashSet();
 * obj.Add(key);
 * obj.Remove(key);
 * bool param_3 = obj.Contains(key);
 */
